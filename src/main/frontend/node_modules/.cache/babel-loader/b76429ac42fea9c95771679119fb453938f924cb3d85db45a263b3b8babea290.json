{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"@/utils/axios\";\nexport default {\n  name: \"PublishActivity\",\n  data() {\n    return {\n      name: \"\",\n      time: \"\",\n      location: \"\",\n      maxParticipants: 3,\n      description: \"\",\n      user: null,\n      showSuccess: false\n    };\n  },\n  methods: {\n    async submitActivity() {\n      const user = JSON.parse(localStorage.getItem(\"user\"));\n      if (!user) {\n        this.error = \"用户信息错误，请重新登录\";\n        return;\n      }\n      const activity = {\n        id: Date.now(),\n        name: this.name,\n        initiator: user.nickname,\n        time: [parseInt(this.year), parseInt(this.month), parseInt(this.day), parseInt(this.hour), parseInt(this.minute)],\n        location: this.location,\n        description: this.description,\n        maxParticipants: parseInt(this.maxParticipants),\n        currentParticipants: 1,\n        participantList: [user.nickname],\n        status: \"审核中\"\n      };\n      try {\n        await axios.post(\"/activities\", activity);\n        this.$router.push(\"/activity-preparation\");\n      } catch (error) {\n        this.error = \"发布活动失败，请稍后再试\";\n      }\n    },\n    closeSuccess() {\n      this.showSuccess = false;\n      this.$router.push(\"/activity-hall\");\n    },\n    setMenu() {\n      this.$store.dispatch(\"setActiveSubMenu\", \"activity\");\n    }\n  },\n  created() {\n    const userData = localStorage.getItem(\"user\");\n    if (userData) {\n      this.user = JSON.parse(userData);\n    }\n    this.setMenu();\n  },\n  watch: {\n    $route() {\n      this.setMenu();\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","time","location","maxParticipants","description","user","showSuccess","methods","submitActivity","JSON","parse","localStorage","getItem","error","activity","id","Date","now","initiator","nickname","parseInt","year","month","day","hour","minute","currentParticipants","participantList","status","post","$router","push","closeSuccess","setMenu","$store","dispatch","created","userData","watch","$route"],"sources":["D:\\Code\\Project\\SNSproject\\SNS\\src\\main\\frontend\\src\\views\\activity\\user\\PublishActivity.vue"],"sourcesContent":["<template>\r\n  <div class=\"publish-activity\">\r\n    <h1>发布活动</h1>\r\n    <form @submit.prevent=\"submitActivity\">\r\n      <div class=\"form-group\">\r\n        <label for=\"name\">活动名称</label>\r\n        <input type=\"text\" id=\"name\" v-model=\"name\" required />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"time\">时间</label>\r\n        <input type=\"datetime-local\" id=\"time\" v-model=\"time\" required />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"location\">地点</label>\r\n        <input type=\"text\" id=\"location\" v-model=\"location\" required />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"maxParticipants\">最多参与人数</label>\r\n        <input\r\n          type=\"number\"\r\n          id=\"maxParticipants\"\r\n          v-model.number=\"maxParticipants\"\r\n          min=\"3\"\r\n          required\r\n        />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"description\">简介</label>\r\n        <textarea\r\n          id=\"description\"\r\n          v-model=\"description\"\r\n          placeholder=\"请在此输入活动简介...\"\r\n          required\r\n        ></textarea>\r\n      </div>\r\n      <button type=\"submit\">发布</button>\r\n    </form>\r\n    <div v-if=\"showSuccess\" class=\"report-success\">\r\n      <div class=\"report-success-box\">\r\n        <p>活动发布成功</p>\r\n        <button @click=\"closeSuccess\" class=\"confirm-button\">确定</button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"@/utils/axios\";\r\n\r\nexport default {\r\n  name: \"PublishActivity\",\r\n  data() {\r\n    return {\r\n      name: \"\",\r\n      time: \"\",\r\n      location: \"\",\r\n      maxParticipants: 3,\r\n      description: \"\",\r\n      user: null,\r\n      showSuccess: false,\r\n    };\r\n  },\r\n  methods: {\r\n    async submitActivity() {\r\n      const user = JSON.parse(localStorage.getItem(\"user\"));\r\n      if (!user) {\r\n        this.error = \"用户信息错误，请重新登录\";\r\n        return;\r\n      }\r\n\r\n      const activity = {\r\n        id: Date.now(),\r\n        name: this.name,\r\n        initiator: user.nickname,\r\n        time: [\r\n          parseInt(this.year),\r\n          parseInt(this.month),\r\n          parseInt(this.day),\r\n          parseInt(this.hour),\r\n          parseInt(this.minute),\r\n        ],\r\n        location: this.location,\r\n        description: this.description,\r\n        maxParticipants: parseInt(this.maxParticipants),\r\n        currentParticipants: 1,\r\n        participantList: [user.nickname],\r\n        status: \"审核中\",\r\n      };\r\n\r\n      try {\r\n        await axios.post(\"/activities\", activity);\r\n        this.$router.push(\"/activity-preparation\");\r\n      } catch (error) {\r\n        this.error = \"发布活动失败，请稍后再试\";\r\n      }\r\n    },\r\n    closeSuccess() {\r\n      this.showSuccess = false;\r\n      this.$router.push(\"/activity-hall\");\r\n    },\r\n    setMenu() {\r\n      this.$store.dispatch(\"setActiveSubMenu\", \"activity\");\r\n    },\r\n  },\r\n  created() {\r\n    const userData = localStorage.getItem(\"user\");\r\n    if (userData) {\r\n      this.user = JSON.parse(userData);\r\n    }\r\n    this.setMenu();\r\n  },\r\n  watch: {\r\n    $route() {\r\n      this.setMenu();\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.publish-activity {\r\n  padding: 20px;\r\n  background: #f9f9f9;\r\n  border-radius: 10px;\r\n  box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\r\n  max-width: 800px;\r\n  margin: 0 auto;\r\n}\r\n\r\n.publish-activity h1 {\r\n  text-align: center;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.form-group label {\r\n  display: block;\r\n  margin-bottom: 5px;\r\n}\r\n\r\n.form-group input,\r\n.form-group textarea {\r\n  width: 100%;\r\n  padding: 10px;\r\n  border-radius: 5px;\r\n  border: 1px solid #ccc;\r\n  box-sizing: border-box;\r\n}\r\n\r\nbutton {\r\n  width: 100%;\r\n  padding: 10px;\r\n  background: #42b983;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  font-size: 16px;\r\n  font-weight: bold;\r\n  transition: background 0.3s;\r\n}\r\n\r\nbutton:hover {\r\n  background: #358a66;\r\n}\r\n\r\n.report-success {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n  background: rgba(0, 0, 0, 0.8);\r\n  color: white;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.report-success-box {\r\n  background: #2c3e50;\r\n  padding: 40px;\r\n  border-radius: 30px;\r\n  text-align: center;\r\n  width: 400px;\r\n}\r\n\r\n.confirm-button {\r\n  padding: 15px 30px;\r\n  background-color: #42b983;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 10px;\r\n  cursor: pointer;\r\n  margin-top: 20px;\r\n  font-size: 16px;\r\n}\r\n\r\n.confirm-button:hover {\r\n  background-color: #358a66;\r\n}\r\n</style>\r\n"],"mappings":";AA+CA,OAAOA,KAAI,MAAO,eAAe;AAEjC,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLD,IAAI,EAAE,EAAE;MACRE,IAAI,EAAE,EAAE;MACRC,QAAQ,EAAE,EAAE;MACZC,eAAe,EAAE,CAAC;MAClBC,WAAW,EAAE,EAAE;MACfC,IAAI,EAAE,IAAI;MACVC,WAAW,EAAE;IACf,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,cAAcA,CAAA,EAAG;MACrB,MAAMH,IAAG,GAAII,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,MAAM,CAAC,CAAC;MACrD,IAAI,CAACP,IAAI,EAAE;QACT,IAAI,CAACQ,KAAI,GAAI,cAAc;QAC3B;MACF;MAEA,MAAMC,QAAO,GAAI;QACfC,EAAE,EAAEC,IAAI,CAACC,GAAG,CAAC,CAAC;QACdlB,IAAI,EAAE,IAAI,CAACA,IAAI;QACfmB,SAAS,EAAEb,IAAI,CAACc,QAAQ;QACxBlB,IAAI,EAAE,CACJmB,QAAQ,CAAC,IAAI,CAACC,IAAI,CAAC,EACnBD,QAAQ,CAAC,IAAI,CAACE,KAAK,CAAC,EACpBF,QAAQ,CAAC,IAAI,CAACG,GAAG,CAAC,EAClBH,QAAQ,CAAC,IAAI,CAACI,IAAI,CAAC,EACnBJ,QAA